{
  "name": "coverjs-server",
  "version": "1.1.0",
  "description": "",
  "main": "src/app.ts",
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "*.sqlite",
      "/public/*"
    ]
  },
  "scripts": {
    "start:dev": "nodemon ./src/server.ts",
    "start": "ts-node ./dist/src/server.js",
    "clean": "rmdir /s /q node_modules",
    "clean:build": "rm -r ./dist",
    "clean:build:win": "rmdir /s /q dist",
    "build": "tsc -p ./ && npm run copyViews",
    "copyViews": "copyfiles --error --up 1 src/views/**/*.twig dist/src/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "await-to-js": "^3.0.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "chokidar": "^3.5.2",
    "concurrently": "^6.3.0",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "email-templates": "^8.0.8",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "fs": "^0.0.1-security",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "node-stream-zip": "^1.15.0",
    "node-unrar-js": "^1.0.3",
    "nodemailer": "^6.7.0",
    "sequelize": "^6.6.5",
    "sharp": "^0.29.1",
    "sqlite3": "^5.0.2",
    "twig": "^1.15.4",
    "unique-string-generator": "^1.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.1",
    "@types/concurrently": "^6.3.0",
    "@types/email-templates": "^8.0.4",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.1",
    "@types/ip": "^1.1.0",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.10.3",
    "@types/nodemailer": "^6.4.4",
    "@types/sharp": "^0.29.2",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "jest": "^27.2.4",
    "nodemon": "^2.0.13",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
